{
  "name": "NightlyShark",
  "private": true,
  "scripts": {
    "front:dev": "cd apps/frontend && npm run ng serve",
    "front:build": "cd apps/frontend && ng build",

    "back:dev": "cd apps/backend && npm run start:dev",
    "back:build": "cd apps/backend && npm run build",

    "dev": "concurrently \"npm run back:dev\" \"npm run front:dev\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
